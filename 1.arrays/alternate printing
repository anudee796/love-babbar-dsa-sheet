#include<bits/stdc++.h>
using namespace std;
int main(){
    int a[5]={0};
    int i=0;
    int j=4;
    int n=5;
    int val=1;
    while(i<j){
        a[i]=val;
        i++;
        val++;
        a[j]=val;
        j--;
        val++;
    }
    a[5/2]=val;
    for(int i=0;i<5;i++){
        cout<<a[i]<<" ";
    }
}
